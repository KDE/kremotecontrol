<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.irkick">
    <method name="isConnected">
      <arg type="b" direction="out"/>
    </method>
    <method name="haveFullList">
      <arg type="b" direction="out"/>
    </method>
    <method name="remotes">
      <arg type="as" direction="out"/>
    </method>
    <method name="buttons">
      <arg type="as" direction="out"/>
      <arg name="theRemote" type="s" direction="in"/>
    </method>
    <method name="stealNextPress">
      <arg name="app" type="s" direction="in"/>
      <arg name="module" type="s" direction="in"/>
      <arg name="method" type="s" direction="in"/>
    </method>
    <method name="dontStealNextPress">
    </method>
    <method name="reloadConfiguration">
    </method>
  </interface>
</node>
