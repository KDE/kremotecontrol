<?xml version="1.0" ?>
<!DOCTYPE remote SYSTEM "remote.dtd">

<remote id="PackBell">
	<name>Packbell Remote Control</name>
	<author>Frank Scheffold</author>
	
	<button id="Help"><name>Help</name></button>
	<button id="Phone"><name>Phone</name></button>
	<button id="Message"><name>Message</name></button>
	<button id="SRS"><name>SRS</name></button>
	
	<button id="CD"><name>Input CD</name></button>
	<button id="Aux1"><name>Input Aux1</name></button>
	<button id="Aux2"><name>Input Aux2</name></button>
	<button id="Aux3"><name>Input Aux3</name></button>
	
	<button id="Up" class="play"><name>Up</name></button>	
	<button id="Down" class="stop"><name>Down</name></button>
	<button id="Left" class="previous"><name>Left</name></button>
	<button id="Right" class="next"><name>Right</name></button>	
	<button id="RMB" class="forward"><name>Right mouse button</name></button>
	<button id="LMB" class="rewind"><name>Left mouse button</name></button>
	
	<button id="Menu"><name>Menu</name></button>
	<button id="Enter" class="enter"><name>Enter</name></button>
	<button id="Display" class="fullscreen"><name>Display</name></button>
	<button id="Mute" class="mute"><name>Mute</name></button>
	<button id="CHUp" class="channelup"><name>Chanel Up</name></button>
	<button id="CHDown" class="channeldown"><name>Chanel Down</name></button>
	<button id="VolUp" class="volumeup"><name>Volume Up</name></button>
	<button id="VolDown" class="volumedown"><name>Volume Down</name></button>
	
	<button id="00" class="number" parameter="0"><name>0</name></button>
	<button id="01" class="number" parameter="1"><name>1</name></button>
	<button id="02" class="number" parameter="2"><name>2</name></button>
	<button id="03" class="number" parameter="3"><name>3</name></button>
	<button id="04" class="number" parameter="4"><name>4</name></button>
	<button id="05" class="number" parameter="5"><name>5</name></button>
	<button id="06" class="number" parameter="6"><name>6</name></button>
	<button id="07" class="number" parameter="7"><name>7</name></button>
	<button id="08" class="number" parameter="8"><name>8</name></button>
	<button id="09" class="number" parameter="9"><name>9</name></button>	
	<button id="Star"><name>Star</name></button>
	<button id="Hash"><name>Hash</name></button>
</remote>